      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./config": "./dist/config.js",
    "./runner-playwright": "./dist/runner-playwright.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.7",
    "cosmiconfig": "^9.0.0",
    "glob": "^11.0.1",
