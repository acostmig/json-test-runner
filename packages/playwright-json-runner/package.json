  "description": "A Playwright package that runs tests using JSON input",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "build-pack": "tsup && npm pack",
    "start": "node dist/index.js",
    "dev": "tsup --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "playwright": "^1.50.1"
  },
  "exports": {
    ".": {
